<?xml version="1.0" encoding="UTF-8"?>
<gui id="Main" width="420" height="185" open="true" resize="true" 
show_title="true" moveble="true" collapsible="false" bring_to_font="false">
	<Collapse text="Sound" seldefault="true" collapsible="true">
		<Button text="Start!" visible="true"/>
		<Button text="Stop!" visible="true"/>
		<Button text="Pause!" visible="true"/>
	</Collapse>
	<Label text="A Simple Text!" visible="true"/>
</gui>

<gui id="Console" width="400" height="250" open="true" resize="false" 
show_title="true" moveble="false" collapsible="true" bring_to_font="true">
	<InputText text="Input" visible="true" history="true" hint="true"
	texthint="Input Command Here!"/>
	<ListBox label="##ForHintCmd" visible="true"/>
	<ChildDialog id="ConsoleText" border="true" horizontal_scroll="true">
		<UnformatedText/>
	</ChildDialog>
</gui>

<gui id="Game Objects" width="420" height="185" open="true" resize="true" 
show_title="true" moveble="true" collapsible="false" bring_to_font="false">
	<Collapse text="Sound" seldefault="true" collapsible="true">
		<TreeNode text="Sounds">
			<Button text="Play" visible="true"/>
			<Button text="Pause" visible="true"/>
			<Label text="Path:" visible="true"/>
			<Label text="3D Pos:" visible="true"/>
			<Label text="Distantion(ToCam):" visible="true"/>
			<Label text="Sound Volume:" visible="true"/>
		</TreeNode>
	</Collapse>
</gui>